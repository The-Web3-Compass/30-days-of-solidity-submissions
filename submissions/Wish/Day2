pragma solidity ^0.8.0;

contract SaveMyname{

  string name;
  string bio;

  function add (string memory _name, string _bio )public {
    name= _name;
    bio =_bio;
}

  function retrieve() public view returns(string memory, string memory){
    return (name,bio);
  }

}
